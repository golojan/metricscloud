{
  "projects": {
    "metrics": {
      "root": "apps/metrics",
      "sourceRoot": "apps/metrics",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/metrics",
            "outputPath": "dist/apps/metrics"
          },
          "configurations": {
            "development": {
              "outputPath": "apps/metrics"
            },
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "metrics:build:development",
            "dev": true
          },
          "configurations": {
            "development": {
              "buildTarget": "metrics:build:development",
              "dev": true
            },
            "production": {
              "buildTarget": "metrics:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "metrics:build:production"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "apps/metrics/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/metrics/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      },
      "tags": []
    },


    "metricsowner": {
      "root": "apps/metricsowner",
      "sourceRoot": "apps/metricsowner",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/metricsowner",
            "outputPath": "dist/apps/metricsowner"
          },
          "configurations": {
            "development": {
              "outputPath": "apps/metricsowner"
            },
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "metricsowner:build:development",
            "dev": true
          },
          "configurations": {
            "development": {
              "buildTarget": "metricsowner:build:development",
              "dev": true
            },
            "production": {
              "buildTarget": "metricsowner:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "metricsowner:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/metricsowner/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      },
      "tags": []
    }
  },
  "libs": {
    "metrics-interfaces": {
      "root": "libs/metrics-interfaces",
      "sourceRoot": "libs/metrics-interfaces/src",
      "projectType": "library"
    },
    "metrics-models": {
      "root": "libs/metrics-models",
      "sourceRoot": "libs/metrics-models/src",
      "projectType": "library"
    },
    "metrics-store": {
      "root": "libs/metrics-store",
      "sourceRoot": "libs/metrics-store/src",
      "projectType": "library"
    }
  },
  "version": 1
}
